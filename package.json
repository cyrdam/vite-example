{
  "name": "vite-example",
  "description": "Repository with vite examples.",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint --ext .ts,tsx .",
    "lint:fix": "eslint --ext .ts,tsx --fix .",
    "format": "prettier \"*.{ts,tsx,css,html}\" --ignore-unknown --write ",
    "format.check": "prettier \"*.{ts,tsx,css,html}\" --ignore-unknown --check",
    "prepare": "husky install"
  },
  "dependencies": {
    "preact": "^10.23.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@preact/preset-vite": "^2.9.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/preact": "^3.2.4",
    "@testing-library/user-event": "^14.5.2",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "eslint": "^8.57.0",
    "eslint-config-preact": "^1.4.0",
    "eslint-config-prettier": "^9.1.0",
    "happy-dom": "^14.12.3",
    "husky": "^9.1.4",
    "jsdom": "^24.1.1",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "vite": "^5.4.0",
    "vitest": "^1.6.0",
    "vitest-dom": "^0.1.1"
  }
}